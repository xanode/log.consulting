---
import { IconsDict } from '@libs/icons';

interface Props {
    className?: string;
    label?: string;
    name: keyof typeof IconsDict;
}
const { className = "", label, name } = Astro.props as Props;
---

<svg 
    aria-label={label}
    aria-hidden={true}
    class:list={[className]}
    viewBox={IconsDict[name].viewbox}
    set:html={IconsDict[name].content}
/>